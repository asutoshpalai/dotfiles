! terminal colors ------------------------------------------------------------

! tangoesque scheme
*background: #111111
*foreground: #babdb6
! Black  + DarkGrey
*color0:  #2E3436
*color8:  #555753
! DarkRed + Red
*color1:  #ff6565
*color9:  #ff8d8d
! DarkGreen + Green
*color2:  #93d44f
*color10: #c8e7a8
! DarkYellow + Yellow
*color3:  #eab93d
*color11: #ffc123
! DarkBlue + Blue
*color4:  #204a87
*color12: #3465a4
! DarkMagenta + Magenta
*color5:  #ce5c00
*color13: #f57900
!DarkCyan + Cyan (both not tango)
*color6:  #89b6e2
*color14: #46a4ff
! LightGrey + White
*color7:  #cccccc
*color15: #ffffff

xterm*dynamicColors:      true
xterm*utf8:               2
xterm*eightBitInput:      true
xterm*saveLines:          512
xterm*scrollKey:          true
xterm*scrollTtyOutput:    false
xterm*scrollBar:          true
xterm*rightScrollBar:     true
xterm*jumpScroll:         true
xterm*multiScroll:        true

URxvt.font: xft:Inconsolata\ for\ Powerline:size=10:pixelsize=14
URxvt.letterSpace: -1
URxvt.scrollBar: false
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.transparent: true
urxvt*depth: 32
!urxvt*background: rgba:0000/0000/02ff/cccc
urxvt*background:       rgba:0000/0000/0000/cccc
URxvt.shading: 15
URxvt.perl-ext-common: resize-font,fullscreen

URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger: C-Up
URxvt*keysym.F11: perl:fullscreen:switch
